<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	
	<servlet>
	<servlet-name>CreateAccountServlet </servlet-name>
	<jsp-file>/views/CreateAccount</jsp-file>
	</servlet>
	
	<servlet-mapping>
	<servlet-name> CreateAccountServlet</servlet-name>
	<url-pattern>/Account</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name> CreateAccount1Servlet</servlet-name>
	<jsp-file> /views/CreateAccount1</jsp-file>
	</servlet>
	
	<servlet-mapping>
	<servlet-name> CreateAccountServlet</servlet-name>
	<url-pattern> /NotCrited</url-pattern>
	</servlet-mapping>
	
	<servlet> 
	<servlet-name>  SinginServlet</servlet-name>
	<jsp-file> /views/SingIn</jsp-file>
	</servlet>
	
	<servlet-mapping>
	<servlet-name> SinginServlet</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>

	
		
	<servlet> 
	<servlet-name>  Singin1Servlet</servlet-name>
	<jsp-file> /views/SingIn1</jsp-file>
	</servlet>
	
	<servlet-mapping>
	<servlet-name> SinginServlet</servlet-name>
	<url-pattern>/NotLogin</url-pattern>
	</servlet-mapping>

	<servlet>
	<servlet-name>SingUpServlet </servlet-name>
	<jsp-file> /views/SingUp</jsp-file>
	</servlet>
	
	
	<servlet-mapping>
	<servlet-name>SingUpServlet </servlet-name>
	<url-pattern>/Singup</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>SingUpServlet1 </servlet-name>
	<jsp-file> /views/SingUp1</jsp-file>
	</servlet>
	
	
	<servlet-mapping>
	<servlet-name>SingUpServlet </servlet-name>
	<url-pattern>/NotSingUp</url-pattern>
	</servlet-mapping>
	
	
	
	
	</web-app>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	
	<servlet>
		<servlet-name>CreateAccountServlet</servlet-name>
		<jsp-file>/WEb_INF/CreateAccount </jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateAccountServlet </servlet-name>
		<url-pattern>/AdminAccount </url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name> CreateAccountServlet1</servlet-name>
		<jsp-file> /WEB_INF/CreateAccount1</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name> CreateAccountServlet1</servlet-name>
		<url-pattern> /AdminAccount2</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	<servlet-name>SinginServlet </servlet-name>
	<jsp-file>/WEb_INF/login </jsp-file>
	</servlet>
	<servlet-mapping>
	<servlet-name>SinginServlet </servlet-name>
	<url-pattern>/UserLogin </url-pattern>
	
	</servlet-mapping>
	
	<servlet> 
	<servlet-name> SinginServlet1</servlet-name>
	<jsp-file>/WEB_INF/Login1 </jsp-file>
	</servlet>
	<servlet-mapping>
	<servlet-name> SinginServlet1</servlet-name>
	<url-pattern>/UserLogin2</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>SingUpServlet </servlet-name>
	<jsp-file>/WEB_INF/SingUp</jsp-file>
	</servlet>
	
	
	<servlet-mapping>
	<servlet-name>SingUpServlet </servlet-name>
	<url-pattern>/UserSingup</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>SingUpServlet1 </servlet-name>
	<jsp-file>/WEB_INF/SingUp1</jsp-file>
	</servlet>
	
	
	<servlet-mapping>
	<servlet-name>SingUpServlet1 </servlet-name>
	<url-pattern>/NotSingUp</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>